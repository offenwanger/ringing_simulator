{
  "name": "RingingRoom Simulator",
  "description": "Simulate Changes",
  "version": "1.4",
  "page_action": {
    "default_title": "Ringing room simulator",
    "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "css": ["main.css"],
      "js": [
        "libs/jquery-3.5.1.min.js",
        "constants.js",
        "interface.js",
        "placeNotationToRowsArray.js",
        "main.js"
      ],
      "matches": [
        "http://*.ringingroom.com/*",
        "https://*.ringingroom.com/*"
      ]
    }
  ],
  "web_accessible_resources": ["icon.svg"]
}
